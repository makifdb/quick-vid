{
    "manifest_version": 3,
    "name": "Quick Vid",
    "description": "Quick Vid is a simple extension that lets you summarize videos",
    "version": "1.0",
    "action": {
      "default_popup": "src/index.html",
      "default_icon": "src/icon.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
  }